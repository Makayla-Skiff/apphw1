@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

    <section class="text-center">
        <h1 class="display-1">Movies</h1>
        <p class="text-center">Best Movies to Watch </p>
    </section>
    <section>

        <section class="row text-left">
            <aside class="col-lg-3 col-md-3 col-sm-12">
                <p class="text-white">Funniest</p>
                <img src="data:image/jpeg;base64," alt="Alternate Text" />
                <img src="data:image/jpeg;base64," alt="Alternate Text" />
                <img src="data:image/jpeg;base64," alt="Alternate Text" />
            </aside>
            <aside class="col-lg-3 col-md-3 col-sm-12">
                <p class="text-white">Romance</p>
                <img src="data:image/jpeg;base64," alt="Alternate Text" />
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRQM5UVSmBom586MOkBgVlC6LB7PqnxxE0bStkDb5-fGZaIbykZ" alt="Alternate Text" />
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRDIUcZ5JqbK2WyNAbGj3Ydt0BTxIzDl_wx9i4fExvuiBDiHqlT" alt="Alternate Text" style="width:200px;height:200px" />
            </aside>
            <aside class="col-lg-3 col-md-3 col-sm-12">
                <p class="text-white">Scary</p>
                <img src="data:image/jpeg;base64," alt="Alternate Text" style="width:300px;height:300px" />
                <img src="data:image/jpeg;base64," alt="Alternate Text" style="width:200px;height:300px" />
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcR6bfUfUjyIqk_etKOxJzfjivOB8aNbe4vbGtYj0XtqJuucMONQGw" alt="Alternate Text" style="width:200px;height:300px" />
            </aside>
        </section>
        <section class="row text-white">
            @foreach (string name in Model.MovieNames)
            {
                <aside class="col-lg-3 col-md-3 col-sm-12">
                    <p style="background-color:coral">@name </p>
                </aside>
            }
        </section>
        <section class="row text-white">
            @foreach (string name in Model.moviename1)
            {
                <aside class="col-lg-3 col-md-3 col-sm-12">
                    <p style="background-color:coral">@name </p>
                </aside>
            }
        </section>
        <section class="row text-white">
            @foreach (string name in Model.movienames2)
            {
                <aside class="col-lg-3 col-md-3 col-sm-12">
                    <p style="background-color:coral">@name </p>
                </aside>
            }
        </section>

    </section>
